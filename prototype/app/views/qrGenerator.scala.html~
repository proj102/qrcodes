@(url: String)

@main("Here is the result:") {
    
    <h1> The requested URL is @url</h1>
<div class="container-fluid">
<!--Creat a 5-column block in Bootstrap grid-->
<div class="span5">
</div>
<div class="span7">

<div id="qrcodeCanvas"></div>


<script>
jQuery('#qrcodeCanvas').qrcode({
		
		
  				text	: "@url"
		
	});

	
		
</script>

</div><!-- end of span7 -->
</div>
    
}
